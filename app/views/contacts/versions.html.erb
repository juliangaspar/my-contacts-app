<div class="container-fluid mt-5">
<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Action</th>
      <th scope="col">Date</th>
      <th scope="col">Handle</th>
      <th scope="col">Changes</th>
    </tr>
  </thead>
  <tbody>
    <% @contact.versions.each do |version| %>
      <tr>
        <th scope="row"><%= version.id %></th>
        <td><%= version.event.humanize %></td>
        <td><%= version.created_at.strftime("%a, %b %e at %l:%M %p") %></td>
        <td><% version.changeset.to_s.humanize%></td>
        <td><%= version.object_changes %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
