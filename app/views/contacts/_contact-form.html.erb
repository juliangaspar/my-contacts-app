<%= simple_form_for (@contact) do |f| %>
    <div class= "form-inputs">
      <%= f.input :first_name, required: true %>
      <%= f.input :last_name, requred: true %>
      <%= f.input :job_title %>
      <%= f.input :company %>
      <%= f.input :contact_email, required: true %>
      <%= f.input :phone_number, required: true %>
      <%= f.input :profile_picture, as: :file %>
    </div>
    <div class="form-actions">
      <%= f.button :submit, "Add", { class: "btn btn-flat" } %>
    </div>
<% end %>

